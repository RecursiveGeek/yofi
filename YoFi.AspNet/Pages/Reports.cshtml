@page
@model YoFi.AspNet.Pages.ReportsModel
@{
    Layout = "_Layout";
    ViewData["Title"] = "Home";
}

@foreach (var report in Model.Reports)
{
    <div class="col-lg-6">
        @await Html.PartialAsync("DisplayReport", report)
    </div>
    
    @if (report.Description == "--")
    {
        <hr />
    }
}